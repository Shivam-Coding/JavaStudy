<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>JavaStudy</servlet-name>
		<servlet-class>com.javaStudy.JavaStudyServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JavaStudy</servlet-name>
		<url-pattern>/JavaStudy</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Delete</servlet-name>
		<servlet-class>com.javaStudy.DeleteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Delete</servlet-name>
		<url-pattern>/Delete</url-pattern>
	</servlet-mapping>
	
	<servlet>
    <servlet-name>AllNotes</servlet-name>
    <servlet-class>com.javaStudy.AllNotesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AllNotes</servlet-name>
    <url-pattern>/AllNotes</url-pattern>
  </servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>Welcome.jsp</welcome-file>
	</welcome-file-list>
</web-app>
